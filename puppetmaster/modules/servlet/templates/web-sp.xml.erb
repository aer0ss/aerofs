  <context-param>
    <param-name>ca_url</param-name>
    <param-value>http://joan.aerofs.com:1029/prod/</param-value>
  </context-param>
  <context-param>
    <param-name>verkehr_host</param-name>
    <param-value>verkehr.aerofs.com</param-value>
  </context-param>
  <context-param>
    <param-name>verkehr_publish_port</param-name>
    <param-value>9293</param-value>
  </context-param>
  <context-param>
    <param-name>verkehr_command_port</param-name>
    <param-value>25234</param-value>
  </context-param>
  <context-param>
    <param-name>verkehr_cacert</param-name>
    <param-value>/etc/ssl/certs/AeroFS_CA.pem</param-value>
  </context-param>

  <!-- initialization listener -->

  <listener>
    <listener-class>com.aerofs.sp.server.sp.SPLifecycleListener</listener-class>
  </listener>

  <!-- servlets -->

  <servlet>
    <servlet-name>ServletSP</servlet-name>
    <servlet-class>com.aerofs.sp.server.sp.SPServlet</servlet-class>
  </servlet>

  <!-- url mapping -->

  <servlet-mapping>
    <servlet-name>ServletSP</servlet-name>
    <url-pattern>/sp</url-pattern>
  </servlet-mapping>
