#!/bin/bash -e -u

include safetynet/constants PUBLIC

######################################################################
#
# Rollsback the SafetyNet client installations to the previously
# backed up state.
#
######################################################################
echo ">> rolling back SafetyNet clients to previous installations"

# Run the SyncDET test that stops the SafetyNet clients and copies the backed up installation over the original
"${SYNCDET}" $SYNCDET_PARAMS -s "${SAFETYNET_SYNCDET_DEPLOY_DIR}/rollback.scn"
