#!/bin/sh
#
# Usage:
#        $ ./retrace <version> <<end
#        (paste obfuscated stack trace here as "heredoc")
#        end
#

source common #for abspath()

VER=$1

if [ x"$VER" == x ]; then
    echo "usage: $0 <VER>"
    exit 1
fi

BASE=`dirname $0`
BASE=`abspath "$BASE"` # needed by proguard's retrace.sh

MAP_FILE="$BASE/aerofs-$VER-prod.map"
$BASE/getmap $VER $BASE
cd "$BASE"/proguard
bin/retrace.sh "$MAP_FILE"
