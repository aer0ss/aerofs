#!/bin/bash -e

echo "Generating histogram data. This might take a while..."
files=$(java -jar $(dirname $0)/../opt/syncstat-tools/syncstat-tools.jar)

ofile=$(echo $files | awk -F' ' '{print $1}')
qfile=$(echo $files | awk -F' ' '{print $2}')

echo "Number of Objects per Device:"
cat $ofile | histogram.py

echo "Number of Queue Entries per Device:"
cat $qfile | histogram.py
